@model App.WebInfo.MVCUI.Models.ReportViewModel

@{
    ViewBag.Title = "Rapor Sayfası";
    ViewBag.isMenuLink = "/";
    ViewBag.SubTitle = "Rapor";
    ViewBag.SubDescription = "Rapor Sayfası";
}
<section class="content">
    <!-- Info boxes -->
    <div class="row">
        <div class="col-xs-12">
            <!-- BEGIN EXAMPLE TABLE PORTLET-->

            <div class="portlet light portlet-fit portlet-datatable bordered">
                <div class="portlet-title">
                    <div class="caption">
                        <i class="icon-layers font-green"></i>
                        <span class="caption-subject font-green sbold uppercase">Personel Listesi</span>

                    </div>
                </div>

               

                <div class="portlet-body">
                    @await Component.InvokeAsync("ReportViewComponet")
                    <table class="table table-striped table-bordered table-hover order-column" id="sample_2" data-servurl="/Report/GetList">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>@Html.DisplayNameFor(m => m.Filter.Adi)</th>
                                <th>@Html.DisplayNameFor(m => m.Filter.Soyadi)</th>
                                <th>@Html.DisplayNameFor(m => m.Filter.SahisNo)</th>
                                <th>@Html.DisplayNameFor(m => m.Filter.AileNo)</th>
                                <th>@Html.DisplayNameFor(m => m.Filter.AnaAdi)</th>
                                <th>@Html.DisplayNameFor(m => m.Filter.BabaAdi)</th>
                                <th>İşlemler</th>
                            </tr>
                        </thead>

                    </table>
                </div>
            </div>
            <!-- END EXAMPLE TABLE PORTLET-->
        </div>
    </div>
</section>